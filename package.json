{
  "name": "lint-filenames",
  "version": "1.0.1",
  "private": true,
  "description": "This github action validates if all files in a given folder match the given regex pattern.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/batista/lint-filenames.git"
  },
  "keywords": [
    "actions",
    "github",
    "lint",
    "linting",
    "filename",
    "file name",
    "filenames",
    "file names",
    "file",
    "extension"
  ],
  "author": "Sergio Batista <dev@se.rg.io>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@typescript-eslint/parser": "^5.61.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.6"
  }
}
